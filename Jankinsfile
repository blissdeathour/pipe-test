pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                sh 'echo \"cloned\"'
            }
        }
        stage('Build') {
            steps {
                sh 'echo \"builded\"'
            }
        }
        stage('Test') {
            steps {
                sh 'echo \"tested\"'
            }
        }
    }
}
